<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
          xmlns:o="urn:schemas-microsoft-com:office:office"
          xmlns:x="urn:schemas-microsoft-com:office:excel"
          xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
          xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Assets_list">
    <Table>
      <Row>
        <Cell><Data ss:Type="String">Name</Data></Cell>
        <Cell><Data ss:Type="String">Description</Data></Cell>
        <Cell><Data ss:Type="String">#Purchased</Data></Cell>
        <Cell><Data ss:Type="String">#Assigned</Data></Cell>
        <Cell><Data ss:Type="String">#Available</Data></Cell>
      </Row>
      <% @nsm_asset_types.each do |asset_type| %>
          <Row>
            <Cell><Data ss:Type="String"><%= asset_type.name %></Data></Cell>
            <Cell><Data ss:Type="String"><%= asset_type.description %></Data></Cell>
            <Cell><Data ss:Type="Number"><%= asset_type.purchased_count %></Data></Cell>
            <Cell><Data ss:Type="Number"><%= asset_type.assigned_count %></Data></Cell>
            <Cell><Data ss:Type="Number"><%= asset_type.available_count %></Data></Cell>
          </Row>
      <% end %>
    </Table>
  </Worksheet>
</Workbook>