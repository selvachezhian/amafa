<p id="notice"><%= notice %></p>

<h1>Nsm Asset Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nsm_asset_types.each do |nsm_asset_type| %>
      <tr>
        <td><%= nsm_asset_type.name %></td>
        <td><%= nsm_asset_type.description %></td>
        <td><%= link_to 'Show', nsm_asset_type %></td>
        <td><%= link_to 'Edit', edit_nsm_asset_type_path(nsm_asset_type) %></td>
        <td><%= link_to 'Destroy', nsm_asset_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nsm Asset Type', new_nsm_asset_type_path %>
