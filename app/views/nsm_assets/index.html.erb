<h3>Nsm Assets</h3>

<div class="container">
  <table class="table table-striped table-hover table-condensed table-bordered">
  <thead>
    <tr>
      <th>Nsm asset</th>
      <th>Code</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nsm_assets.each do |nsm_asset| %>
      <tr>
        <td><%= nsm_asset.nsm_asset_type.name %></td>
        <td><%= nsm_asset.code %></td>
        <td><%= link_to 'Show', nsm_asset %></td>
        <td><%= link_to 'Edit', edit_nsm_asset_path(nsm_asset) %></td>
        <td><%= link_to 'Destroy', nsm_asset, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>


<%= link_to 'New Nsm Asset', new_nsm_asset_path, class: 'btn btn-info' %>
</div>